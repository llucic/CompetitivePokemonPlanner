<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="android:textViewStyle">@android:style/TextAppearance.DeviceDefault</item>
    </style>

    <style name="MyActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/XPrimary</item>
    </style>

    <style name="OmegaRubyTheme" parent="AppTheme">

    </style>

    <style name="AlphaSapphireTheme" parent="AppTheme">

    </style>

    <style name="XTheme" parent="AppTheme">
        <item name="android:actionBarStyle">@style/XActionBar</item>
    </style>

    <style name="YTheme" parent="AppTheme">
        <item name="android:actionBarStyle">@style/YActionBar</item>
    </style>

    <style name="XActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/XPrimary</item>
    </style>
    <style name="YActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/YPrimary</item>
    </style>
    <style name="ORActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/YPrimary</item>
    </style>
    <style name="ASActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/XPrimary</item>
    </style>

</resources>
